<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">
	<diskStore path="user.home/web_ehcache/" />
	<defaultCache
			maxElementsInMemory="30000"
			eternal="false"
			timeToIdleSeconds="3600"
			timeToLiveSeconds="3600"
			overflowToDisk="true"
			diskPersistent="false"
			diskExpiryThreadIntervalSeconds="100"
			memoryStoreEvictionPolicy="LRU" />
	<cache name="publicCache"
		maxElementsInMemory="30000"
		eternal="true"
		overflowToDisk="true"
		timeToIdleSeconds="0"
		timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="stringCache"
		maxElementsInMemory="30000"
		eternal="true"
		overflowToDisk="true"
		timeToIdleSeconds="0"
		timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="listCache"
		maxElementsInMemory="300000"
		eternal="true"
		overflowToDisk="true"
		timeToIdleSeconds="0"
		timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="shareInfoCache"
		maxElementsInMemory="30000"
		eternal="true"
		overflowToDisk="true"
		timeToIdleSeconds="0"
		timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="shareHolderInfoCache"
		maxElementsInMemory="30000"
		eternal="true"
		overflowToDisk="true"
		timeToIdleSeconds="0"
		timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="viewCache"
		maxElementsInMemory="30000"
		eternal="true"
		overflowToDisk="true"
		timeToIdleSeconds="0"
		timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
</ehcache>
